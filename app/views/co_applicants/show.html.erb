  <tr><td><%= model_class.human_attribute_name(:locality_of_residence) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.locality_of_residence %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:ease_of_location) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.ease_of_location %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:construction_of_residence) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.construction_of_residence %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:comments_of_exteriors) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.comments_of_exteriors %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:carpet_area_in_sq_feet) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.carpet_area_in_sq_feet %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:interior_condition) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.interior_condition %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:asset_seen_at_residence) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.asset_seen_at_residence %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:potrait_on_wall) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.potrait_on_wall %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:applicant_residing_detail) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.applicant_residing_detail %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:application_availibility_time) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.application_availibility_time %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:no_of_family_member_in_the_house) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.no_of_family_member_in_the_house %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:verifiers_remark) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.verifiers_remark %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:refer_to_bank) %>:</td>
  <td><%= @co_applicant.co_applicant_servey.refer_to_bank %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:pin) %>:</td>
  <td><%#= @co_applicant.pincode.name %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:country_state) %>:</td>
  <td><%= @co_applicant.country_state %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:country_city) %>:</td>
  <td><%= @co_applicant.country_city %></td></tr>

  <tr><td><%= model_class.human_attribute_name(:country_name) %>:</td>
  <td><%= @co_applicant.country_name %></td></tr>

</table>
</div>
<% end %>
<% end %>
