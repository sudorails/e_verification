
<table class="table table-striped">
  <% ClientVerification.find_all_by_tab_id(current_user.tab, :order => 'created_at DESC').each do |c| %>
 <tr><% if c.co_applicant_business.status=="submitted" %>
  <td><strong><%= c.co_applicant_business.applicant_name %></strong>&nbsp;&nbsp;&nbsp;&nbsp;
     <% time1 = Time.now %> <% time2 = c.created_at %>
     <p>
      <small style ="color:silver;padding-left:10px;padding-right:10px;margin-left:20px;">
          <%= distance_of_time_in_words(time1, time2) %> ago <br>
          <%= c.co_applicant_business.address %>
      </small>
     </p>
  </td>
  <td><%= link_to "Verify",new_co_applicant_work_detail_path(:customer_co_applicant_id=>c.co_applicant_business.id, :tab_id=> current_user.tab.id), :class=>"label label-info" %></td>
    <% end %>
  <% end %></tr></table>


