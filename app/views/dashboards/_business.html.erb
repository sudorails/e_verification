<div class="span6" style ="padding:20px;border-radius:5px;border:0px solid #000000;text-align:center;margin-right:-170px;">
 <h5>Business verification</h5>
<table class="table table-striped table-condensed table-bordered">
  <% @submitted_businesses.each do |c| %>
 <tr><% if c.submitted? %>
  <td><strong><%= c.customer.applicant_name %></strong>&nbsp;&nbsp;&nbsp;&nbsp;
     <% time1 = Time.now %> <% time2 = c.created_at %>
      <small style ="color:silver;padding-left:10px;padding-right:10px;margin-left:20px;">
          <%= distance_of_time_in_words(time1, time2) %> ago <br>
      </small>
     <p style ="color:silver;font-size:11px;">
          <%= c.address %>
     </p>
  </td>
  <td><%= link_to "Verify",new_work_servey_path(:customer_business_id => c.id, :tab_id => current_user.tab.id), :class=>"label label-info" %></td>
    <% end %>
  <% end %></tr></table>
 <%= render "co_applicant_business" %>
</div>
