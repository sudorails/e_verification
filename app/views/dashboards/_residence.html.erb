<div class="span4" style ="width:500px;padding:20px;border-radius:5px;border:0px solid #000000;text-align:center">
 <h5>Residencial verification</h5>
<table class="table table-striped">
  <% CustomerVerification.find_all_by_tab_id(current_user.tab, :order => 'created_at DESC').each do |c| %>
 <tr><% if c.customer.status=="submitted" %>
  <td><strong><%= c.customer.applicant_name %></strong>&nbsp;&nbsp;&nbsp;&nbsp;
      <strong><%= c.customer.application_ref_no %></strong>
     <% time1 = Time.now %> <% time2 = c.created_at %>
      <small style ="color:silver;padding-left:10px;padding-right:10px;margin-left:20px;">
          <%= distance_of_time_in_words(time1, time2) %> ago <br>
      </small>
     <p style ="color:silver;font-size:11px;">
        <%= c.customer.address %>
     </p>
  </td>
  <td><%= link_to "Verify",new_servey_path(:customer_id=>c.customer.id, :tab_id=>current_user.tab.id), :class=>"label label-info" %></td>
    <% end %>
  <% end %></tr></table>
<%= render "co_applicant" %>
 <!--<table class="table table-striped">
 <% customer_data.each do |c| %>
   <tr><td><%= c.customer.applicant_name %></td><tr>
 <% end %>
 </table>-->

</div>
